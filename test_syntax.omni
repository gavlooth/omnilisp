;; 1. Standard Scheme-style
(define (add x y) (+ x y))
(println (add 10 20))

;; 2. Scheme-style with Slot brackets
(define (sub [x y]) (- x y))
(println (sub 50 5))

;; 3. Scheme-style with Type Hints and Return Type
(define (mul x {Int} y {Int}) {Int} (* x y))
(println (mul 6 7))

;; 4. Scheme-style with Slot and Type Hints
(define (div [x {Int} y {Int}]) {Int} (/ x y))
(println (div 100 2))

;; 5. Original Clojure-style (still works)
(define fact [n] (if (<= n 1) 1 (* n (fact (- n 1)))))
(println (fact 5))